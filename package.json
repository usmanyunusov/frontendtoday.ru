{
  "private": true,
  "scripts": {
    "start": "npm-run-all dev",
    "build": "ELEVENTY_ENV=prod npm-run-all clean rollup eleventy",
    "clean": "rm -rf dist",
    "dev": "npm-run-all clean rollup --parallel --race watch:rollup watch:eleventy",
    "rollup": "node build.js",
    "watch:eleventy": "eleventy --serve --quiet",
    "watch:rollup": "chokidar \"src/assets/{scripts,styles}/**/*.{js,scss}\" --silent -c \"npm run rollup\"",
    "eleventy": "eleventy --quiet"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "9.8.6",
    "chalk": "^4.1.0",
    "chokidar-cli": "^2.1.0",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "eleventy": "^1.0.6",
    "fancy-log": "^1.3.3",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-csso": "^4.0.0",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "pretty-data": "^0.40.0",
    "rollup": "^2.27.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.26.11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 8",
    "not op_mini all",
    "iOS > 7",
    "Firefox > 20"
  ],
  "devDependencies": {
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3"
  }
}
